import java.util.*;
class LCM{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=0;
        for(int i=1;i<=a;i++){
            if(a%i==0&&b%i==0)
                c=i;
        }
        int l=a*b/c;
        System.out.print("LCM is"+l);

    }
}
