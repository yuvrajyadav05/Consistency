class HelloWorld {
    public static int sumofevennumbers(int start,int end){
        int total=0;
        for(int i=start;i<=end;i++){
            if(i%2==0){
                total += i;
            }
        }
        return total;
    }
    public static void main(String[] args) {
        int start=1;
        int end=10;
        int result=sumofevennumbers(start,end);
        System.out.println(result);
       
    }
}
